{
  "name": "type-realm",
  "version": "11.0.0",
  "description": "Typescript support and class utils for realm-js",
  "homepage": "https://github.com/ibilux/type-realm",
  "keywords": [
    "realm",
    "realm-js",
    "node",
    "react-native",
    "typescript",
    "class",
    "schema",
    "decorators"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "README.md",
    "coverage/badge-*.svg"
  ],
  "repository": {
    "type": "git",
    "url": "github.com/ibilux/type-realm"
  },
  "author": "ibilux <ibilux@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint -c .eslintrc.cjs index.ts",
    "lint:fix": "eslint -c .eslintrc.cjs --fix index.ts",
    "format": "prettier --write \"index.ts\"",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage && jest-coverage-badges"
  },
  "devDependencies": {
    "@types/istanbul-lib-report": "^3.0.0",
    "@types/jest": "^29.2.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "bson": "4.7.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-coverage-badges": "^1.0.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "directories": {
    "lib": "lib"
  }
}
